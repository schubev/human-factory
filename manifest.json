{
	"name": "Human Factory",
	"version": "0.5.0",
	"description": "Insert random values in fields for first names, last names, birthdatesâ€¦",
	"manifest_version": 2,
	"permissions": ["activeTab"],
	"background": {
		"scripts": ["dist/background.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["dist/content.js"],
			"all_frames": true
		}
	],
	"commands": {
		"fill-focused-field": {
			"suggested_key": {
				"default": "Alt+Shift+J",
				"mac": "MacCtrl+J"
			},
			"description": "Fill focused input field"
		}
	}
}
