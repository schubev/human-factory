{
	"name": "Human Factory",
	"version": "1.0.1",
	"description": "Insert random values in fields for first names, last names, birthdatesâ€¦",
	"manifest_version": 2,
	"permissions": ["activeTab"],
	"homepage_url": "https://github.com/schubev/human-factory",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"commands": {
		"fill-focused-field": {
			"suggested_key": {
				"default": "Alt+Shift+J",
				"mac": "MacCtrl+J"
			},
			"description": "Fill focused input field"
		}
	}
}
