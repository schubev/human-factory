{
	"name": "Human Factory",
	"version": "0.6.0",
	"description": "Insert random values in fields for first names, last names, birthdatesâ€¦",
	"manifest_version": 2,
	"permissions": ["activeTab"],
	"homepage_url": "https://github.com/schubev/human-factory",
	"background": {
		"scripts": ["dist/background.js"],
		"persistent": false
	},
	"icons": {
		"16": "dist/icon-16.png",
		"48": "dist/icon-48.png",
		"128": "dist/icon-128.png"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["dist/content.js"],
			"all_frames": true
		}
	],
	"commands": {
		"fill-focused-field": {
			"suggested_key": {
				"default": "Alt+Shift+J",
				"mac": "MacCtrl+J"
			},
			"description": "Fill focused input field"
		}
	}
}
